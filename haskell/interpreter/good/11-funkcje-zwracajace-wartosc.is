int fib(int num) {
    if (num == 0 || num == 1) {
        return num;
    }
    
    array<int> fibSeries(num+1);
    fibSeries[0] = 0;
    fibSeries[1] = 1;
    
    int i;
    for (i from 2 to (num + 1)) {
        fibSeries[i] = fibSeries[(i-1)] + fibSeries[(i-2)];
    }
    
    return fibSeries[num];
}



int main() {

    print("Piata liczba Fibonacciego to: ");
    print(fib(5));

    return 0;
}